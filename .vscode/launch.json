{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "iOS Web",
      "type": "chrome",
      "request": "attach",
      "port": 9000,
      "url": "http://localhost:8000/*",
      "webRoot": "${workspaceRoot}/src"
    }
  ]
}
